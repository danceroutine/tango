{
  "name": "@danceroutine/tango-migrations",
  "version": "0.1.0",
  "description": "Migration runner and SQL helpers for Tango (PostgreSQL first)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["tango", "migrations", "database", "sql", "postgres"],
  "author": "Pedro Del Moral Lopez",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "tsdown": "^0.4.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.6"
  }
}

