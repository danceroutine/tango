{
  "name": "tango",
  "version": "0.0.0",
  "private": true,
  "description": "Typescript Web Framework inspired by Django and DRF!",
  "license": "MIT",
  "author": "Pedro Del Moral Lopez",
  "type": "module",
  "scripts": {
    "prelint": "pnpm format",
    "lint": "pnpm run lint:ox && pnpm run lint:eslint && pnpm format:check",
    "lint:ox": "oxlint --format stylish",
    "lint:eslint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "pnpm -r run typecheck",
    "build": "pnpm -r run build",
    "docs:dev": "pnpm --filter @tango/docs dev",
    "docs:build": "pnpm --filter @tango/docs build",
    "docs:preview": "pnpm --filter @tango/docs preview",
    "changeset": "changeset",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "changeset:publish": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.14.0",
    "eslint-plugin-oxlint": "^1.25.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "oxlint": "^1.25.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.4.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  },
  "packageManager": "pnpm@9.13.2",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}
